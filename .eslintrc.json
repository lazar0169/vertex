{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "no-case-declarations": "off",
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": "off",
        "no-undef": "off",
        "no-var": "error",
        "eqeqeq": "error"
    }
}